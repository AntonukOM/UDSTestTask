@using Test.WebUI.Models

@section scripts
{
    <script src="~/Scripts/pages/users.js"></script>
}

@{
    ViewBag.Title = "All";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<UserModel> users = ViewBag.Users;
}
@Html.Partial("LogOutPartial")

<h3 class="text-success lead">User Name: @ViewBag.UserName</h3>
<table class="table table-hover table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Company</th>
            <th>Email</th>
            <th style="width:150px">Phone</th>
            <th>Tags</th>
        </tr>
    </thead>
    <tbody id="list">
       @Html.Partial("UserListPartial", users)
    </tbody>
</table>